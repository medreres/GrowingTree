f [ -n \"$SFML_NETWORK\" ]\nthen\n    require \"network\"\nfi\n\nif [ -n \"$SFML_WINDOW\" ]\nthen\n    require \"window\"\nfi\n\nif [ -n \"$SFML_GRAPHICS\" ]\nthen\n    require \"graphics\"\nfi\n\n                \n","shellPath":"/bin/sh","type":"com.apple.buildphase.shell-script"}],"buildRules":[],"dependencies":[],"guid":"0a0508d0faae0eafc85c1f313293364ffab29565ab7c8d370a318d614cea168b","name":"GrowingTree","predominantSourceCodeLanguage":"Xcode.SourceCodeLanguage.C-Plus-Plus","productReference":{"guid":"0a0508d0faae0eafc85c1f313293364f5f88827bc6461a8d499efdb6a4c5131e","name":"GrowingTree.app","type":"product"},"productTypeIdentifier":"com.apple.product-type.application","provisioningSourceData":[{"bundleIdentifierFromInfoPlist":"me.${PRODUCT_NAME:rfc1034identifier}","configurationName":"Debug","legacyTeamID":"","provisioningStyle":0},{"bundleIdentifierFromInfoPlist":"me.${PRODUCT_NAME:rfc1034identifier}","configurationName":"Release","legacyTeamID":"","provisioningStyle":0}],"type":"standard"}